{
  "name": "ether-backend",
  "version": "1.0.0",
  "description": "",
  "directories": {
    "build": "./build"
  },
  "scripts": {
    "test": "bash test/scripts/runTestsCoverage.sh",
    "build": "truffle compile",
    "deploy": "truffle migrate --network rinkeby",
